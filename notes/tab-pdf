    <script src="/sites/all/themes/shared/science_jcore/js/modernizr.js?version=3.3.1"></script>
    <script type="text/javascript" src="//assets.adobedtm.com/926659a1689cc9fca96760b67cca714ae48f0584/satelliteLib-a9d46808a35b7b8034adfab1d8207407222304d5.js"></script>
